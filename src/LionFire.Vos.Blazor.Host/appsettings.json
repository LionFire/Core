{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "tcp": {
        "type": "NLogViewer",
        "address": "tcp4://localhost:4505/"
      },     
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "tcp"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
